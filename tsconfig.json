{
  "compilerOptions": {
    "outDir": "src",
    "alwaysStrict": true,
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2019"
    ],
    "module": "CommonJS",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "stripInternal": true,
    "target": "ES2019",
    "paths": {
      "@cdk/*": [
        "./cdk/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@common/*": [
        "./src/common/*"
      ],
      "@tests/*": [
        "./tests/*"
      ]
    },
    "baseUrl": ".",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "jest",
      "node"
    ]
  },
  "include": [
    "src/**/*.ts",
    "webpack.config.ts",
    "cdk/**/*.ts",
    "scripts/swagger.ts",
    "jest.setup.ts"
  ],
  "exclude": [
    "cdk.out",
    "./cdk.out",
    "./node_modules",
    "./dist",
    "./build"
  ],
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
